<?xml version="1.0" encoding="UTF-8"?>
<!-- 子窗口的结构模板，根节点的名称随意，符合XML规范即可 -->
<ChildWindowTemplate>
    <ChildWindow valign="center" halign="center" child_window_margin="12,36,12,12">
        <HBox padding="12,0,0,0" valign="top" height="36">
            <Label name="child_window_name" text="ChildWindow1" margin="2,0,2,0" valign="center" mouse_enabled="false"/>
            <Label text="帧率FPS:" valign="center" mouse_enabled="false"/>
            <Label name="label_fps" text="0000" width="42" margin="4,0,4,0" valign="center" mouse_enabled="false"/>
            <CheckBox class="checkbox_1" name="fps_paint" height="28" width="auto" text="绘制" valign="center" selected="true"/>
            <CheckBox class="checkbox_1" name="child_fullscreen" height="28" width="auto" text="全屏显示" valign="center" margin="8,0,0,0" selected="false"/>
        </HBox>
    </ChildWindow>
</ChildWindowTemplate>