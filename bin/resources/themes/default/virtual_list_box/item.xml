<?xml version="1.0" encoding="UTF-8"?>
<Window>
    <ListBoxItem class="listitem">
        <HBox mouse_enabled="false" padding="10,5,10,5">
            <Control name="control_img" width="auto" height="auto" margin="0,0,10,0" mouse_enabled="false"/>
            <VBox margin="0,3,0,5" mouse_enabled="false">
                <HBox height="auto" mouse_enabled="false">
                    <Label name="label_title" width="stretch" mouse_enabled="false"/>
                    <Button class="btn_recycle" name="btn_del" width="auto" height="auto" tooltip_text="删除"/>
                </HBox>
                <Control height="stretch" mouse_enabled="false"/>
                <Progress class="progress_horizontal_blue" name="progress" value="30" mouse_enabled="false"/>
            </VBox>
        </HBox>
    </ListBoxItem>
</Window>